<p id="notice"><%= notice %></p>

<h1>Listing Students</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Regno</th>
      <th>Dept</th>
      <th>Batch</th>
      <th>User</th>
      <th>Hoa</th>
      <th>Sa</th>
      <th>Hoal</th>
      <th>Sal</th>
      <th>Dob</th>
      <th>Gender</th>
      <th>Fathersname</th>
      <th>Guardian</th>
      <th>Community</th>
      <th>Religionn</th>
      <th>Tenth</th>
      <th>Twelth</th>
      <th>Fathersoccupiton</th>
      <th>Identification</th>
      <th>Blood</th>
      <th>Dote</th>
      <th>Mobile</th>
      <th>Pmobile</th>
      <th>Addr1</th>
      <th>Addr2</th>
      <th>Mark</th>
      <th>Ten</th>
      <th>Two</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.name %></td>
        <td><%= student.regno %></td>
        <td><%= student.dept %></td>
        <td><%= student.batch %></td>
        <td><%= student.user %></td>
        <td><%= student.hoa %></td>
        <td><%= student.sa %></td>
        <td><%= student.hoal %></td>
        <td><%= student.sal %></td>
        <td><%= student.dob %></td>
        <td><%= student.gender %></td>
        <td><%= student.fathersname %></td>
        <td><%= student.guardian %></td>
        <td><%= student.community %></td>
        <td><%= student.religionn %></td>
        <td><%= student.tenth %></td>
        <td><%= student.twelth %></td>
        <td><%= student.fathersoccupiton %></td>
        <td><%= student.identification %></td>
        <td><%= student.blood %></td>
        <td><%= student.dote %></td>
        <td><%= student.mobile %></td>
        <td><%= student.pmobile %></td>
        <td><%= student.addr1 %></td>
        <td><%= student.addr2 %></td>
        <td><%= student.mark %></td>
        <td><%= student.ten %></td>
        <td><%= student.two %></td>
        <td><%= link_to 'Show', student %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

