<p id="notice"><%= notice %></p>

<h1>Listing Attendance Masters</h1>

<table>
  <thead>
    <tr>
      <th>Semester</th>
      <th>Date</th>
      <th>Today</th>
      <th>Subject</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendance_masters.each do |attendance_master| %>
      <tr>
        <td><%= attendance_master.semester %></td>
        <td><%= attendance_master.date %></td>
        <td><%= attendance_master.today %></td>
        <td><%= attendance_master.subject %></td>
        <td><%= attendance_master.total %></td>
        <td><%= link_to 'Show', attendance_master %></td>
        <td><%= link_to 'Edit', edit_attendance_master_path(attendance_master) %></td>
        <td><%= link_to 'Destroy', attendance_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attendance master', new_attendance_master_path %>
